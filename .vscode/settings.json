{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"editor.formatOnSave": "explicit",
		"source.organizeImports": "explicit",
		"source.fixAll": "explicit",
		"source.fixAll.organizeImports": "explicit"
	},
	"prettier.singleQuote": true,
	"prettier.semi": false,
	"prettier.trailingComma": "es5",
	"prettier.bracketSpacing": true,
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"notebook.output.wordWrap": true,
	"notebook.stickyScroll.mode": "flat",
	"jupyter.themeMatplotlibPlots": true,
	"data.preview.theme": "dense.dark",
	"material-icon-theme.folders.theme": "classic",
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter"
	},
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true,
		"editor.formatOnSave": true,
		"source.organizeImports": true,
		"source.fixAll": true,
		"source.fixAll.organizeImports": true
	},
	"cSpell.words": ["aiohappyeyeballs", "aiohttp", "dotenv", "llms"]
}
